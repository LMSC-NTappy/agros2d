body, h1, h2, h3, h4, h5, table {
	font-family: {{FONTFAMILY}}, sans-serif;
	font-size: {{FONTSIZE}}pt;
	color: rgb(50, 50, 50);
}

body {
	padding: 2pt;
	margin: 0;
}

h1 {
	padding: 2pt;
	margin: 0;
	color: rgb(0, 0, 0);
}

h2 {
	padding: 1pt;
	margin: 0;
}

table, tr, td {
	text-align: left;
	/*border: 1pt solid black;*/
}

.maintable {
	width: 100%;
}

.section {
	padding: 2pt 0pt 10pt 2pt;
	margin: 0;
}

.subsection {
	padding: 1pt 0pt 1pt 4pt;
	margin: 0;
}
