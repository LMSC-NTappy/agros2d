body, h1, h2, h3, h4, h5, table {
	font-family: {{FONTFAMILY}};
	font-size: {{FONTSIZE}}pt;
	color: rgb(0, 0, 0);
	margin: 0;
	padding: 0;
}

body {
	padding: 5pt;
}

h4 {
	font-weight: bold;
	color: rgb(0, 0, 0);
}

h5 {
	font-weight: bold;
	color: rgb(66, 66, 66);
}

table, tr, td {
	text-align: left;
}

.maintable {
	width: 100%;
}

.section {
	padding-top: 5pt;
	padding-bottom: 5pt;
}

.subsection {
	padding-left: 10pt;
}
