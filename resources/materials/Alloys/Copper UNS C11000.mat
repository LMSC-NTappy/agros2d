      <material:constant value="8889.5"/>
      <material:dependence>
        <material:function body="def agros2d_material(t) :&#xA;&#x9;t = t + 273.15&#xA;&#x9;if t >= 83.0 and t &lt;= 1373.0 :&#xA;&#x9;&#x9;return -1.475948e-04*t*t -3.630825e-01*t +9.008531e+03&#xA;&#x9;else :&#xA;&#x9;&#x9;return 1.000000e+100&#xA;" interval_from="-180" interval_to="1090"/>
