<?xml version="1.0"?>
<module>
    <general id ="rf" name="RF">
        <description>RF.</description>
        <properties steadystate="0" harmonic="2" transient="0" />
    </general>

    <constants>
        <item id="EPS0" value="8.854e-12" />
        <item id="MU0" value="1.2566371e-06" />
    </constants>
    
    <material>
        <item id="rf_permittivity" name="Permittivity" shortname="epsr" unit="-" />
        <item id="rf_permeability" name="Permeability" shortname="mur" unit="-" />  
        <item id="rf_conductivity" name="Conductivity" shortname="gamma" unit="S/m" />
        <item id="rf_current_density_external_real" name="Current dens. - ext. - real" shortname="Jer" unit="A/m2" />
        <item id="rf_current_density_external_imag" name="Current dens. - ext. - imag" shortname="Jei" unit="A/m2" />
        
    	<harmonic>
    		<material id="rf_permittivity" />
    		<material id="rf_permeability" />
        	<material id="rf_conductivity" />
        	<material id="rf_current_density_external_real" />
        	<material id="rf_current_density_external_imag" />
    	
        	<matrix i="1" j="1" planar="- 1 / (mur * MU0) * (udx * vdx + udy * vdy) + (2 * PI * f)^2 * epsr * EPS0 * uval * vval" 
        						axi="" />
        	<matrix i="2" j="2" planar="- 1 / (mur * MU0) * (udx * vdx + udy * vdy) + (2 * PI * f)^2 * epsr * EPS0 * uval * vval" 
        						axi="" />
		</harmonic>
    </material>

    <boundary>
    	<item id="rf_electric_field_real" name="Electric field - real" shortname="Er" unit="V/m" />
    	<item id="rf_electric_field_imag" name="Electric field - imag" shortname="Ei" unit="V/m" />
    	<item id="rf_magnetic_field_real" name="Magnetic field - real" shortname="Hr" unit="A/m" />
    	<item id="rf_magnetic_field_imag" name="Magnetic field - imag" shortname="Hi" unit="A/m" />
    	<item id="rf_surface_current_real" name="Surface current - real" shortname="Jr" unit="A/m2" />
    	<item id="rf_surface_current_imag" name="Surface current - imag" shortname="Ji" unit="A/m2" />
    	<item id="rf_port_power" name="Power" shortname="P" unit="W" />
    	<item id="rf_port_phase" name="Phase" shortname="alpha" unit="deg." />
    	
 		<harmonic>
       		<boundary id="rf_electric_field" name="Electric field">
            	<item id="rf_electric_field_real" />
            	<item id="rf_electric_field_imag" />
				
				<essential i="1" id="rf_electric_field_real" />
				<essential i="2" id="rf_electric_field_imag" />
			</boundary> 
			
       		<boundary id="rf_magnetic_field" name="Magnetic field">
            	<item id="rf_magnetic_field_real" />
            	<item id="rf_magnetic_field_imag" />
				
				<vector i="1" planar="Hr * vval" axi="Hr * x * vval" />
				<vector i="2" planar="Hi * vval" axi="Hi * x * vval" />
			</boundary>  
			
       		<boundary id="rf_surface_current" name="Surface current">
            	<item id="rf_surface_current_real" />
            	<item id="rf_surface_current_imag" />
				
				<vector i="1" planar="Hr * vval" axi="Hr * x * vval" />
				<vector i="2" planar="Hi * vval" axi="Hi * x * vval" />
			</boundary>  			
			
       		<boundary id="rf_port" name="Port">
            	<item id="rf_port_power" />
            	<item id="rf_port_phase" />
            	
            	<matrix i="1" j="2" planar="uval * vval" axi="uval * vval" />
        		<matrix i="2" j="1" planar="uval * vval" axi="uval * vval" />
        	</boundary>  
        	
			<boundary id="acoustic_matched_boundary" name="Matched boundary" >
			</boundary>  
		</harmonic>
    </boundary>	     

    <localvariable>
        <item id="rf_electric_field_real" name="Electric field - real" shortname="Er" unit="V/m" scalar="1">
            <harmonic planar_scalar="value1" axi_scalar="value1" />
            <transient />
        </item>
        <item id="rf_electric_field_imag" name="Electric field - imag" shortname="Ei" unit="V/m" scalar="1">
            <harmonic planar_scalar="value2" axi_scalar="value2" />
            <transient />
        </item>
        <item id="rf_displacement_real" name="Displacement - real" shortname="Dr" unit="C/m2" scalar="1">
            <harmonic planar_scalar="epsr * EPS0 * value1" axi_scalar="epsr * EPS0 * value1" />
            <transient />
        </item>
        <item id="rf_displacement_imag" name="Displacement - imag" shortname="Di" unit="C/m2" scalar="1">
            <harmonic planar_scalar="epsr * EPS0 * value2" axi_scalar="epsr * EPS0 * value2" />
            <transient />
        </item>
        <item id="rf_surface_current_real" name="Current density - real" shortname="Jr" unit="A/m2" scalar="1">
            <harmonic planar_scalar="gamma * value1" axi_scalar="gamma * value1" />
            <transient />
        </item>
        <item id="rf_surface_current_imag" name="Current density - imag" shortname="Ji" unit="A/m2" scalar="1">
            <harmonic planar_scalar="gamma * value2" axi_scalar="gamma * value2" />
            <transient />
        </item>
        <item id="rf_magnetic_field_real" name="Magnetic field - real" shortname="Hr" unit="A/m" scalar="0">
            <harmonic planar_comp_x="- 1.0 / (2.0 * PI * f * mur * MU0) * dy2" planar_comp_y="1.0 / (2.0 * PI * f * mur * MU0) * dx2" axi_comp_r="- 1.0 / (2.0 * PI * f * mur * MU0) * dy2" axi_comp_z="1.0 / (2.0 * PI * f * mur * MU0) * dx2" />
            <transient />
        </item>
        <item id="rf_magnetic_field_imag" name="Magnetic field - imag" shortname="Hi" unit="A/m" scalar="0">
            <harmonic planar_comp_x="1.0 / (2.0 * PI * f * mur * MU0) * dy1" planar_comp_y="1.0 / (2.0 * PI * f * mur * MU0) * dx1" axi_comp_r="1.0 / (2.0 * PI * f * mur * MU0) * dy1" axi_comp_z="1.0 / (2.0 * PI * f * mur * MU0) * dx1" />
            <transient />
        </item>
        <item id="rf_flux_density_real" name="Flux density - real" shortname="Br" unit="T" scalar="0">
            <harmonic planar_comp_x="- 1.0 / (2.0 * PI * f) * dy2" planar_comp_y="1.0 / (2.0 * PI * f) * dx2" axi_comp_r="- 1.0 / (2.0 * PI * f) * dy2" axi_comp_z="1.0 / (2.0 * PI * f) * dx2" />
            <transient />
        </item>
        <item id="rf_flux_density_imag" name="Flux density - imag" shortname="Bi" unit="T" scalar="0">
            <harmonic planar_comp_x="1.0 / (2.0 * PI * f) * dy1" planar_comp_y="1.0 / (2.0 * PI * f) * dx1" axi_comp_r="1.0 / (2.0 * PI * f) * dy1" axi_comp_z="1.0 / (2.0 * PI * f) * dx1" />
            <transient />
        </item>
        <item id="rf_poynting_vector" name="Poyting vector" shortname="N" unit="W/m2" scalar="0">
            <harmonic planar_comp_x="0.5 / (2.0 * PI * f * mur * MU0) * (value2 * dx1 - value1 * dx2)" planar_comp_y="0.5 / (2.0 * PI * f * mur * MU0) * (value2 * dy1 - value1 * dy2)" axi_comp_r="0.5 / (2.0 * PI * f * mur * MU0) * (value2 * dx1 - value1 * dx2)" axi_comp_z="0.5 / (2.0 * PI * f * mur * MU0) * (value2 * dy1 - value1 * dy2)" />
            <transient />
        </item>
        <item id="rf_permittivity" name="Permittivity" shortname="epsr" unit="-" scalar="1">
            <harmonic planar_scalar="epsr" axi_scalar="epsr" />
            <transient />
        </item>
        <item id="rf_permeability" name="Permeability" shortname="mur" unit="-" scalar="1">
            <harmonic planar_scalar="mur" axi_scalar="mur" />
            <transient />
        </item>
        <item id="rf_conductivity" name="Conductivity" shortname="gamma" unit="S/m" scalar="1">
            <harmonic planar_scalar="gamma" axi_scalar="gamma" />
            <transient />
        </item>
    </localvariable>

    <view>
        <scalarvariable>
            <harmonic default="rf_electric_field_real">
                <item id="rf_electric_field_real" />
                <item id="rf_electric_field_imag" />
                <item id="rf_surface_current_real" />
                <item id="rf_surface_current_imag" />
                <item id="rf_displacement_real" />
                <item id="rf_displacement_imag" />
                <item id="rf_magnetic_field_real" />
                <item id="rf_magnetic_field_imag" />
                <item id="rf_flux_density_real" />
                <item id="rf_flux_density_imag" />
                <item id="rf_poynting_vector" />
                <item id="rf_permittivity" />
                <item id="rf_permeability" />
                <item id="rf_conductivity" />
            </harmonic>
            <transient/>
        </scalarvariable>

        <vectorvariable>
            <harmonic default="rf_magnetic_field_real"/>
                <item id="rf_magnetic_field_real" />
                <item id="rf_magnetic_field_imag" />
                <item id="rf_flux_density_real" />
                <item id="rf_flux_density_imag" />
                <item id="rf_poynting_vector" />
            <transient/>
        </vectorvariable>
    </view>

    <pointvalue>
        <harmonic>
            <item id="rf_electric_field_real" />
            <item id="rf_electric_field_imag" />
            <item id="rf_surface_current_real" />
            <item id="rf_surface_current_imag" />
            <item id="rf_displacement_real" />
            <item id="rf_displacement_imag" />
            <item id="rf_magnetic_field_real" />
            <item id="rf_magnetic_field_imag" />
            <item id="rf_flux_density_real" />
            <item id="rf_flux_density_imag" />
            <item id="rf_poynting_vector" />
            <item id="rf_permittivity" />
            <item id="rf_permeability" />
            <item id="rf_conductivity" />
        </harmonic>
        <transient/>
    </pointvalue>

    <volumeintegral>
        <item id="volume" name="Volume" shortname="V" unit="m3">
            <harmonic planar="1.0" axi="2.0 * PI * x" />
            <transient/>
        </item>
        <item id="surface" name="Cross section" shortname="S" unit="m2">
            <harmonic planar="1.0" axi="1.0" />
            <transient/>
        </item>
    </volumeintegral>

    <surfaceintegral>
        <item id="length" name="Length" shortname="l" unit="m">
            <harmonic planar="1.0" axi="1.0" />
            <transient/>
        </item>
        <item id="surface" name="Surface" shortname="S" unit="m2">
            <harmonic planar="1.0" axi="2.0 * PI * x" />
            <transient/>
        </item>
    </surfaceintegral>
</module>
