<?xml version="1.0"?>
<module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="module.xsd">
    <general id="acoustic" name="Acoustic">
        <description>Acoustics is the interdisciplinary science that deals with the study of all mechanical waves in gases, liquids, and solids.</description>
        <properties steadystate="0" harmonic="2" transient="0"></properties>    
    </general>

    <constants>
        <item id="P0" value="20e-6"></item>
    </constants>

    <material>
        <item id="acoustic_density" name="Mass density" shortname="rho" unit="kg/m3"></item>
        <item id="acoustic_speed" name="Speed of sound" shortname="vel" unit="m/s"></item>  
        
    	<harmonic>
            <material id="acoustic_density"></material>
            <material id="acoustic_speed"></material>

            <matrix i="1" j="1" planar="1 / rho * (udx * vdx + udy * vdy) - (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * uval * vval" axi="1 / rho * r * (udr * vdr + udz * vdz) - r * (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * uval * vval" symmetric="0"></matrix>
            <matrix i="2" j="2" planar="1 / rho * (udx * vdx + udy * vdy) - (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * uval * vval" axi="1 / rho * r * (udr * vdr + udz * vdz) - r * (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * uval * vval" symmetric="0"></matrix>
            <vector i="1" j="1" planar="1 / rho * (updx * vdx + updy * vdy) - (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * upval * vval" axi="1 / rho * r * (updr * vdr + updz * vdz) - r * (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * upval * vval"></vector>
            <vector i="2" j="2" planar="1 / rho * (updx * vdx + updy * vdy) - (2 * PI * f) * (2 * PI * f) / (rho *vel * vel) * upval * vval" axi="1 / rho * r * (updr * vdr + updz * vdz) - r * (2 * PI * f) * (2 * PI * f) / (rho * vel * vel) * upval * vval"></vector>

        </harmonic>
    </material>

    <boundary>
    	<item id="acoustic_pressure_real" name="Pressure - real" shortname="pr" unit="Pa"></item>
    	<item id="acoustic_pressure_imag" name="Pressure - imag" shortname="pi" unit="Pa"></item>
    	<item id="acoustic_normal_acceleration_real" name="Normal acceleration - real" shortname="anr" unit="m/s2"></item>
    	<item id="acoustic_normal_acceleration_imag" name="Normal acceleration - imag" shortname="ani" unit="m/s2"></item>
    	<item id="acoustic_impedance" name="Impedance boundary condition" shortname="Z0" unit="Pa·s/m"></item>
 
        <harmonic default="acoustic_pressure">
            <boundary id="acoustic_pressure" name="Pressure">
                <item id="acoustic_pressure_real"></item>
                <item id="acoustic_pressure_imag"></item>

                <essential i="1" planar="pr" axi="pr"></essential>
                <essential i="2" planar="pi" axi="pi"></essential>
            </boundary>

            <boundary id="acoustic_normal_acceleration" name="Normal acceleration">
                <item id="acoustic_normal_acceleration_real"></item>
                <item id="acoustic_normal_acceleration_imag"></item>

                <vector i="1" j="1" planar="- anr * vval" axi="- anr * r * vval"></vector>
                <vector i="2" j="2" planar="- ani * vval" axi="- ani * r * vval"></vector>
            </boundary>

            <boundary id="acoustic_impedance" name="Impedance boundary condition">
                <item id="acoustic_impedance"></item>

                <matrix i="1" j="2" planar="- 2 * PI * f / Z0 * uval * vval" axi="- r * 2 * PI * f / Z0 * uval * vval"></matrix>
                <matrix i="2" j="1" planar="2 * PI * f / Z0 * uval * vval" axi="r * 2 * PI * f / Z0 * uval * vval"></matrix>
                <vector i="1" j="2" planar="- 2 * PI * f / Z0 * upval * vval" axi="- r * 2 * PI * f / Z0 * upval * vval"></vector>
                <vector i="2" j="1" planar="2 * PI * f / Z0 * upval * vval" axi="r * 2 * PI * f / Z0 * upval * vval"></vector>
            </boundary>
        </harmonic>
    </boundary>	    

    <localvariable>
        <item id="acoustic_pressure" name="Pressure" shortname="p" unit="Pa" scalar="1">
            <harmonic planar_scalar="sqrt(value1^2 + value2^2)" axi_scalar="sqrt(value1^2 + value2^2)"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_pressure_real" name="Pressure - real" shortname="pr" unit="Pa" scalar="1">
            <harmonic planar_scalar="value1" axi_scalar="value1"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_pressure_imag" name="Pressure - imag" shortname="pi" unit="Pa" scalar="1">
            <harmonic planar_scalar="value2" axi_scalar="value2"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_pressure_level" name="Pressure level" shortname="SPL" unit="dB" scalar="1">
            <harmonic planar_scalar="20 * log10(sqrt(value1^2 + value2^2) / P0)" axi_scalar="20 * log10(sqrt(value1^2 + value2^2) / P0)"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_local_acceleration_real" name="Local acceleration - real" shortname="ar" unit="m/s2">
            <harmonic planar_comp_x="- dx1 / rho" planar_comp_y="- dy1 / rho" axi_comp_r="- dr1 / rho" axi_comp_z="- dz1 / rho"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_local_acceleration_imag" name="Local acceleration - imag" shortname="ai" unit="m/s2">
            <harmonic planar_comp_x="- dx2 / rho" planar_comp_y="- dy2 / rho" axi_comp_r="- dr2 / rho" axi_comp_z="- dz2 / rho"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_density" name="Mass density" shortname="rho" unit="kg/m3" scalar="1">
            <harmonic planar_scalar="rho" axi_scalar="rho"></harmonic>
            <transient></transient>
        </item>
        <item id="acoustic_speed" name="Speed of sound" shortname="vel" unit="m/s" scalar="1">
            <harmonic planar_scalar="vel" axi_scalar="vel"></harmonic>
            <transient></transient>
        </item>
    </localvariable>

    <view>
        <scalarvariable>
            <harmonic default="acoustic_pressure_real">
                <item id="acoustic_pressure"></item>
                <item id="acoustic_pressure_real"></item>
                <item id="acoustic_pressure_imag"></item>
                <item id="acoustic_local_acceleration"></item>
                <item id="acoustic_local_acceleration_real"></item>
                <item id="acoustic_local_acceleration_imag"></item>
                <item id="acoustic_pressure_level"></item>
                <item id="acoustic_density"></item>
                <item id="acoustic_speed"></item>
            </harmonic>
            <transient></transient>
        </scalarvariable>

        <vectorvariable>
            <harmonic default="acoustic_local_acceleration_real">
                <item id="acoustic_local_acceleration_real"></item>
            </harmonic>
            <transient></transient>
        </vectorvariable>
    </view>

    <pointvalue>
        <harmonic>
            <item id="acoustic_pressure"></item>
            <item id="acoustic_pressure_real"></item>
            <item id="acoustic_pressure_imag"></item>
            <item id="acoustic_local_acceleration"></item>
            <item id="acoustic_local_acceleration_real"></item>
            <item id="acoustic_local_acceleration_imag"></item>
            <item id="acoustic_pressure_level"></item>
            <item id="acoustic_density"></item>
            <item id="acoustic_speed"></item>
        </harmonic>
        <transient></transient>
    </pointvalue>

    <volumeintegral>
        <item id="volume" name="Volume" shortname="V" unit="m3">
            <harmonic planar="1.0" axi="2.0 * PI * r"></harmonic>
            <transient></transient>
        </item>
        <item id="surface" name="Cross section" shortname="S" unit="m2">
            <harmonic planar="1.0" axi="1.0"></harmonic>
            <transient></transient>
        </item>
    </volumeintegral>

    <surfaceintegral>
        <item id="length" name="Length" shortname="l" unit="m">
            <harmonic planar="1.0" axi="1.0"></harmonic>
            <transient></transient>
        </item>
        <item id="surface" name="Surface" shortname="S" unit="m2">
            <harmonic planar="1.0" axi="2.0 * PI * r"></harmonic>
            <transient></transient>
        </item>
    </surfaceintegral>
</module>
