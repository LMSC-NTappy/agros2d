    <boundary>
        <item id="magnetic_potential_real" name="Vector potential - real" shortname="Ar" unit="Wb/m"></item>
        <item id="magnetic_potential_imag" name="Vector potential - imag" shortname="Ai" unit="Wb/m"></item>
        <item id="magnetic_surface_current_real" name="Surface current - real" shortname="Jr" unit="A/m"></item>
        <item id="magnetic_surface_current_imag" name="Surface current - imag" shortname="Ji" unit="A/m"></item>

        <steadystate default="magnetic_potential">
            <boundary id="magnetic_potential" name="Magnetic potential">
                <item id="magnetic_potential_real"></item>

                <essential i="1" planar="Ar" axi="Ar"></essential>
            </boundary>

            <boundary id="magnetic_surface_current" name="Surface current">
                <item id="magnetic_surface_current_real"></item>

                <vector i="1" j="1" planar="-Jr * vval" axi="-Jr * vval"></vector>
            </boundary>
        </steadystate>

        <transient default="magnetic_potential">
            <boundary id="magnetic_potential" name="Magnetic potential">
                <item id="magnetic_potential_real"></item>

                <essential i="1" planar="Ar" axi="Ar"></essential>
            </boundary>

            <boundary id="magnetic_surface_current" name="Surface current">
                <item id="magnetic_surface_current_real"></item>

                <vector i="1" j="1" planar="-Jr * vval" axi="-Jr * vval"></vector>
            </boundary>
        </transient>

        <harmonic default="magnetic_potential">
            <boundary id="magnetic_potential" name="Magnetic potential">
            <item id="magnetic_potential_real"></item>
            <item id="magnetic_potential_imag"></item>

                <essential i="1" planar="Ar" axi="Ar"></essential>
                <essential i="2" planar="Ai" axi="Ai"></essential>
            </boundary>

            <boundary id="magnetic_surface_current" name="Surface current">
                <item id="magnetic_surface_current_real"></item>
                <item id="magnetic_surface_current_imag"></item>

                <vector i="1" j="1" planar="-Jr * vval" axi="-Jr * vval"></vector>
                <vector i="2" j="2" planar="-Ji * vval" axi="-Ji * vval"></vector>
            </boundary>
        </harmonic>
    </boundary>

    <localvariable>
        <item id="magnetic_potential_module" name="Vector potential" shortname="A" unit="Wb/m" scalar="1">
            <steadystate planar_scalar="sqrt(value1^2 + value2^2)" axi_scalar="sqrt(value1^2 + value2^2)"></steadystate>
            <harmonic planar_scalar="sqrt(value1^2 + value2^2)" axi_scalar="sqrt(value1^2 + value2^2)"></harmonic>
            <transient planar_scalar="sqrt(value1^2 + value2^2)" axi_scalar="sqrt(value1^2 + value2^2)"></transient>
        </item>
        <item id="magnetic_potential_real" name="Vector potential - real" shortname="Ar" unit="Wb/m" scalar="1">
            <steadystate planar_scalar="value1" axi_scalar="value1"></steadystate>
            <harmonic planar_scalar="value1" axi_scalar="value1"></harmonic>
            <transient planar_scalar="value1" axi_scalar="value1"></transient>
        </item>
        <item id="magnetic_potential_imag" name="Vector potential - imag" shortname="Ai" unit="Wb/m" scalar="1">
            <steadystate></steadystate>
            <harmonic planar_scalar="value2" axi_scalar="value2"></harmonic>
            <transient></transient>
        </item>
        <item id="magnetic_flux_density_module" name="Flux density" shortname="B" unit="T" scalar="1">
            <steadystate></steadystate>
            <harmonic planar_scalar="sqrt(dy1^2 + dx1^2 + dy2^2 + dx2^2)" axi_scalar="sqrt(dz1^2 + (dr1 + value1 / (r + 1e-12))^2 + dz2^2 + (dr2 + value2 / (r + 1e-12))^2)"></harmonic>
            <transient></transient>
        </item>
        <item id="magnetic_flux_density_real" name="Flux density - real" shortname="Br" unit="T" scalar="0">
            <steadystate planar_comp_x="dy1" planar_comp_y="- dx1" axi_comp_r="- dz1" axi_comp_z="dr1 + value1 / (r + 1e-12)"></steadystate>
            <harmonic planar_comp_x="dy1" planar_comp_y="- dx1" axi_comp_r="- dz1" axi_comp_z="dr1 + value1 / (r + 1e-12)"></harmonic>
            <transient planar_comp_x="dy1" planar_comp_y="- dx1" axi_comp_r="- dz1" axi_comp_z="dr1 + value1 / (r + 1e-12)"></transient>
        </item>
        <item id="magnetic_flux_density_imag" name="Flux density - imag" shortname="Bi" unit="T" scalar="0">
            <steadystate></steadystate>
            <harmonic planar_comp_x="dy2" planar_comp_y="- dx2" axi_comp_r="- dz2" axi_comp_z="dr2 + value2 / (r + 1e-12)"></harmonic>
            <transient></transient>
        </item>
        <item id="magnetic_magnetic_field_module" name="Magnetic field" shortname="H" unit="A/m" scalar="1">
            <steadystate></steadystate>
            <harmonic planar_scalar="sqrt(dy1^2 + dx1^2 + dy2^2 + dx2^2) / (mur * MU0)" axi_scalar="sqrt(dz1^2 + (dr1 + value1 / (r + 1e-12))^2 + dz2^2 + (dr2 + value2 / (r + 1e-12))^2) / (mur * MU0)"></harmonic>
            <transient></transient>
        </item>
        <item id="magnetic_magnetic_field_real" name="Magnetic field - real" shortname="Hr" unit="A/m" scalar="0">
            <steadystate planar_comp_x="dy1 / (mur * MU0)" planar_comp_y="- dx1 / (mur * MU0)" axi_comp_r="- dz1 / (mur * MU0)" axi_comp_z="(dr1 + value1 / (r + 1e-12)) / (mur * MU0)"></steadystate>
            <harmonic planar_comp_x="dy1 / (mur * MU0)" planar_comp_y="- dx1 / (mur * MU0)" axi_comp_r="- dz1 / (mur * MU0)" axi_comp_z="(dr1 + value1 / (r + 1e-12)) / (mur * MU0)"></harmonic>
            <transient planar_comp_x="dy1 / (mur * MU0)" planar_comp_y="- dx1 / (mur * MU0)" axi_comp_r="- dz1 / (mur * MU0)" axi_comp_z="(dr1 + value1 / (r + 1e-12)) / (mur * MU0)"></transient>
        </item>
        <item id="magnetic_magnetic_field_imag" name="Magnetic field - imag" shortname="Hi" unit="A/m" scalar="0">
            <steadystate></steadystate>
            <harmonic planar_comp_x="dy2 / (mur * MU0)" planar_comp_y="- dx2 / (mur * MU0)" axi_comp_r="- dz2 / (mur * MU0)" axi_comp_z="(dr2 + value2 / (r + 1e-12)) / (mur * MU0)"></harmonic>
            <transient></transient>
        </item>
        <item id="magnetic_energy_density" name="Energy density" shortname="wm" unit="J/m3" scalar="1">
            <steadystate planar_scalar="0.5 * (dy1^2 + dx1^2) / (mur * MU0)" axi_scalar="0.5 * (dz1^2 + (dr1 + value1 / (r + 1e-12))^2) / (mur * MU0)"></steadystate>
            <harmonic planar_scalar="(dy1^2 + dx1^2 + dy2^2 + dx2^2) / (mur * MU0)" axi_scalar="(dz1^2 + (dr1 + value1 / (r + 1e-12))^2 + dz2^2 + (dr2 + value2 / (r + 1e-12))^2) / (mur * MU0)"></harmonic>
            <transient planar_scalar="0.5 * (dy1^2 + dx1^2) / (mur * MU0)" axi_scalar="0.5 * (dz1^2 + (dr1 + value1 / (r + 1e-12))^2) / (mur * MU0)"></transient>
        </item>
        <item id="magnetic_permeability" name="Permeability" shortname="mur" unit="-" scalar="1">
            <steadystate planar_scalar="mur" axi_scalar="mur"></steadystate>
            <harmonic planar_scalar="mur" axi_scalar="mur"></harmonic>
            <transient planar_scalar="mur" axi_scalar="mur"></transient>
        </item>
        <item id="magnetic_remanence" name="Remanent flux density" shortname="Brm" unit="T" scalar="0">
            <steadystate planar_comp_x="Brm * cos((Bra/180*PI))" planar_comp_y="Brm * sin((Bra/180*PI))" axi_comp_r="Brm * cos((Bra/180*PI))" axi_comp_z="Brm * sin((Bra/180*PI))"></steadystate>
            <harmonic planar_comp_x="Brm * cos((Bra/180*PI))" planar_comp_y="Brm * sin((Bra/180*PI))" axi_comp_r="Brm * cos((Bra/180*PI))" axi_comp_z="Brm * sin((Bra/180*PI))"></harmonic>
            <transient planar_comp_x="Brm * cos((Bra/180*PI))" planar_comp_y="Brm * sin((Bra/180*PI))" axi_comp_r="Brm * cos((Bra/180*PI))" axi_comp_z="Brm * sin((Bra/180*PI))"></transient>
        </item>
        <item id="magnetic_velocity" name="Velocity" shortname="v" unit="m/s" scalar="0">
            <steadystate planar_comp_x="vx - va * y" planar_comp_y="vy + va * x" axi_comp_r="0" axi_comp_z="vy"></steadystate>
            <harmonic planar_comp_x="vx - va * y" planar_comp_y="vy + va * x" axi_comp_r="0" axi_comp_z="vy"></harmonic>
            <transient planar_comp_x="vx - va * y" planar_comp_y="vy + va * x" axi_comp_r="0" axi_comp_z="vy"></transient>
        </item>
        <item id="magnetic_current_density_external_modul" name="Current dens. - ext." shortname="Je" unit="A/m2" scalar="1">
            <steadystate planar_scalar="sqrt(Jer^2+Jei^2)" axi_scalar="sqrt(Jer^2+Jei^2)"></steadystate>
            <harmonic planar_scalar="sqrt(Jer^2+Jei^2)" axi_scalar="sqrt(Jer^2+Jei^2)"></harmonic>
            <transient planar_scalar="sqrt(Jer^2+Jei^2)" axi_scalar="sqrt(Jer^2+Jei^2)"></transient>
        </item>
        <item id="magnetic_current_density_external_real" name="Current dens. - ext. - real" shortname="Jer" unit="A/m2" scalar="1">
            <steadystate planar_scalar="Jer" axi_scalar="Jer"></steadystate>
            <harmonic planar_scalar="Jer" axi_scalar="Jer"></harmonic>
            <transient planar_scalar="Jer" axi_scalar="Jer"></transient>
        </item>
        <item id="magnetic_current_density_external_imag" name="Current dens. - ext. - imag" shortname="Jei" unit="A/m2" scalar="1">
            <steadystate></steadystate>
            <harmonic planar_scalar="Jei" axi_scalar="Jei"></harmonic>
            <transient></transient>
        </item>
    </localvariable>

